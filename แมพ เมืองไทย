-- ตารางสำหรับเก็บจำนวนเงินและรอบ
local moneyRounds = {
    [100] = 1,       -- 100 เงิน = 1 รอบ
    [1000] = 10,     -- 1000 เงิน = 10 รอบ
    [10000] = 100,   -- 10000 เงิน = 100 รอบ
    [100000] = 1000, -- 100000 เงิน = 1000 รอบ
    [1000000] = 10000, -- 1000000 เงิน = 10000 รอบ
    [10000000] = 100000 -- 10000000 เงิน = 100000 รอบ
}

local selectedAmount = nil -- ตัวแปรสำหรับเก็บจำนวนเงินที่เลือก

-- ตารางสำหรับตัวเลือกใน Dropdown ที่เป็นคำแบบไทย
local dropdownOptions = {
    ["1ร้อยบาท"] = 100,
    ["1พันบาท"] = 1000,
    ["1หมื่นบาท"] = 10000,
    ["1แสนบาท"] = 100000,
    ["1ล้านบาท"] = 1000000,
    ["10ล้านบาท"] = 10000000
}

-- Dropdown สำหรับเลือกจำนวนเงินแบบคำภาษาไทย
Money:NewDropdown("เลือกจำนวนเงิน", "DropdownInf", {"1ร้อยบาท", "1พันบาท", "1หมื่นบาท", "1แสนบาท", "1ล้านบาท", "10ล้านบาท"}, function(currentOption)
    selectedAmount = dropdownOptions[currentOption] -- แปลงตัวเลือกภาษาไทยเป็นจำนวนเงิน
end)

-- ฟังก์ชันเพื่อยิงรีโมท
local function fireRemote(roundsToFire)
    local args = {
        [1] = "Sell"
    }

    for i = 1, roundsToFire do
        game:GetService("ReplicatedStorage").House:FireServer(unpack(args))
    end

    print("ยิงรีโมทไปทั้งหมด " .. roundsToFire .. " รอบ สำหรับเงิน " .. selectedAmount .. " บาท")
end

-- ปุ่มสำหรับยิงรีโมท
Money:NewButton("กดยิง", "ยิงรีโมทตามจำนวนเงินที่เลือก", function()
    if selectedAmount then
        local rounds = moneyRounds[selectedAmount] -- ค้นหารอบจากตาราง

        if rounds then
            fireRemote(rounds) -- ยิงรีโมทตามจำนวนรอบ
        else
            warn("ไม่สามารถค้นหาเงินที่เลือกได้!")
        end
    else
        warn("กรุณาเลือกจำนวนเงินก่อน!")
    end
end)


-- ตัวแปรชื่อผู้เล่นสำหรับส่งเงิน
_G.moneyPlayerName = ""
_G.amount = 0
_G.autoSend = false

-- กล่องข้อความสำหรับชื่อผู้เล่นในฟังก์ชันส่งเงิน
Money:NewTextBox("ชื่อผู้เล่นสำหรับส่งเงิน", "", function(playerName)
    if playerName and playerName ~= "" then
        _G.moneyPlayerName = playerName
        print("ชื่อผู้เล่นสำหรับส่งเงินถูกตั้งค่าเป็น:", _G.moneyPlayerName)
    else
        print("กรุณากรอกชื่อผู้เล่นสำหรับส่งเงิน")
    end
end)

-- กล่องข้อความสำหรับจำนวนเงิน
Money:NewTextBox("จำนวนเงิน", "ใส่จำนวนเงิน", function(amount)
    local numAmount = tonumber(amount)
    if numAmount and numAmount > 0 then
        _G.amount = numAmount
        print("จำนวนเงินถูกตั้งค่าเป็น:", _G.amount)
    else
        print("กรุณากรอกจำนวนเงินที่ถูกต้อง")
    end
end)

-- ฟังก์ชันส่งเงินอัตโนมัติ
Money:NewToggle("ส่งเงินออโต้(ทุก1วิ)", '?', function(isEnabled)
    _G.autoSend = isEnabled
    while _G.autoSend do
        wait(1)
        if _G.moneyPlayerName ~= "" and _G.amount > 0 then
            local args = {
                [1] = _G.moneyPlayerName, -- ชื่อผู้เล่นที่ถูกกำหนดไว้สำหรับส่งเงิน
                [2] = _G.amount -- จำนวนเงิน
            }
            game:GetService("ReplicatedStorage").DonateEvent:FireServer(unpack(args))
        end
    end
end)

-- ปุ่มสำหรับส่งเงินครั้งเดียว
Money:NewButton("กดส่งรอบเดียว", "ส่งรอบเดียว", function()
    if _G.moneyPlayerName ~= "" and _G.amount > 0 then
        local args = {
            [1] = _G.moneyPlayerName, -- ชื่อผู้เล่นที่ถูกกำหนดไว้สำหรับส่งเงิน
            [2] = _G.amount -- จำนวนเงิน
        }
        game:GetService("ReplicatedStorage").DonateEvent:FireServer(unpack(args))
    else
        print("กรุณากรอกชื่อผู้เล่นและจำนวนเงินให้ครบถ้วน")
    end
end)

