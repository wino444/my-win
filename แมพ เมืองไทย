-- ตัวแปรเปิด/ปิด NoCooldown
local NoCooldown = false

-- สร้างตาราง hooks เพื่อเก็บ event hooks
local hooks = {}

-- ค้นหา ProximityPrompt และจัดการ Event
for _, object in pairs(workspace:GetDescendants()) do
    if object:IsA("ProximityPrompt") then
        -- บันทึกฟังก์ชันเดิมไว้
        local originalAction = object.Triggered:Connect(function(player)
            if NoCooldown then
                print("ProximityPrompt Triggered by: ", player.Name)
                object.HoldDuration = 0 -- ทำให้กดแล้วติดทันที
            else
                object.HoldDuration = 1 -- ค่าเริ่มต้น (หรือเปลี่ยนเป็นค่าตามเดิมของเกม)
            end
        end)

        -- ใส่ event เข้าไปในรายการ hooks
        table.insert(hooks, originalAction)
    end
end

-- ปุ่มเปิด/ปิด NoCooldown
ch:NewToggle("ไม่คูลดาวน์ (ProximityPrompt)", "?", function(win)
    NoCooldown = win
    print("NoCooldown: " .. tostring(NoCooldown))
end)






