Money:NewToggle("ออโต้ฟาร์มเงิน(ได้น้อย)", "?", function(Value)
    _G.auto = Value
    while _G.auto do
        wait(1) -- Added delay to prevent rapid execution

        local planeFolder = workspace:FindFirstChild("PlaneFolder")
        
        if planeFolder then
            for _, v in pairs(planeFolder:GetDescendants()) do
                if v:IsA("TouchTransmitter") then
                    -- Ensure firetouchinterest is defined
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) -- 0 is touch
                    wait(0.1) -- Short delay to avoid rapid firing
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
                end
            end
        else
            warn("PlaneFolder not found in workspace.")
        end
    end
end)


-- Hook into the toggle
Money:NewToggle("ออโต้ฟาร์มเงิน(โรงพยาบาล)", '?', function(value)
    _G.boom = value
    while _G.boom do
        wait(1) -- Adding a delay to avoid rapid firing and reduce server load
        clickAllDetectors()
    end
end)

