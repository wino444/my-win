                                           --function
local function debugPrintChildren(parent)
    local maxChildren = 10  -- Number of children to print
    local children = parent:GetChildren()
    print("Children of", parent.Name, ":")
    for i, child in ipairs(children) do
        if i > maxChildren then break end
        print(i, child.Name)
    end
end

local function clickAllDetectors()
    local index = 470  -- Adjust index as needed
    local parentFolder = workspace:GetChildren()[index]
    
    if parentFolder then
        print("Checking parent folder:", parentFolder.Name)
        local pillsFolder = parentFolder:FindFirstChild("PillsFolder")
        
        if pillsFolder then
            print("PillsFolder found.")
            local colors = {"Blue", "White", "Yellow"}

            for _, color in ipairs(colors) do
                local colorFolder = pillsFolder:FindFirstChild(color)
                
                if colorFolder then
                    print(color .. " folder found. Checking for ClickDetectors...")
                    for _, v in pairs(colorFolder:GetDescendants()) do
                        if v:IsA("ClickDetector") then
                            -- Ensure fireclickdetector is defined
                            fireclickdetector(v)
                        end
                    end
                else
                    warn(color .. " folder not found in PillsFolder.")
                end
            end
        else
            warn("PillsFolder not found in the specified parent folder.")
        end
    else
        warn("Parent folder not found in workspace.")
    end
end

