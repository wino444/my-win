local ccamount = 0
 
	 for i,v in pairs(game:GetDescendants()) do
		 if v:IsA("ClickDetector") then
				ccamount = ccamount + 1
			 fireclickdetector(v)
		 end
	 end

 wait();
 
 Notify({
 Description = "Fired " .. ccamount .. " amount of click detectors";
 Title = "Nameless Admin";
 Duration = 7;
 
 });
 end)

Money:NewButton("ซื้อปืน", "??", function()
    -- Define the index of the child in the workspace
    local index = 428

    -- Get the child from workspace using the index
    local targetChild = workspace:GetChildren()[index]

    if targetChild then
        -- Attempt to get the "Head" and "ClickDetector" from the target child
        local head = targetChild:FindFirstChild("Head")
        if head then
            local clickDetector = head:FindFirstChildOfClass("ClickDetector")
            if clickDetector then
                -- Ensure fireclickdetector is defined
                fireclickdetector(clickDetector)
                print("ClickDetector fired successfully.")
            else
                warn("ClickDetector not found in Head.")
            end
        else
            warn("Head not found in the target child.")
        end
    else
        warn("Target child not found in workspace at index " .. index)
    end
end)


